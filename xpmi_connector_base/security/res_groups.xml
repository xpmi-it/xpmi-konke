<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Creating a Group Category -->
    <record id="connector_category" model="ir.module.category">
        <field name="name">Connector</field>
        <field name="sequence">1</field>
    </record>

    <!-- Adding a Group to the Group Category -->
    <record id="group_connector_master" model="res.groups">
        <field name="name">Master</field>
        <field name="category_id" ref="connector_category"/>
    </record>
    <record id="group_connector_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="connector_category"/>
    </record>

</odoo>