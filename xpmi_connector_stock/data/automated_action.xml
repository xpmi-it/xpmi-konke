<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="ir_cron_export_stock" model="ir.cron">
        <field name="name">Connector: Export Stock</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_type">hours</field>
        <field name="interval_number">1</field>
        <field name="state">code</field>
        <field name="code">model.ir_cron_export_stock()</field>
        <field name="model_id" ref="da_connector_base.model_connector_setting"/>
    </record>

    <record id="ir_cron_update_all_connector_stock_line" model="ir.cron">
        <field name="name">Connector: Update All Connector Stock Line</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_type">hours</field>
        <field name="interval_number">24</field>
        <field name="state">code</field>
        <field name="code">model.update_all_connector_stock_line()</field>
        <field name="model_id" ref="da_connector_base.model_connector_setting"/>
    </record>

</odoo>
